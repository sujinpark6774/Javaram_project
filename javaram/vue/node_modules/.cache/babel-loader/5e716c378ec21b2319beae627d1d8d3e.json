{"ast":null,"code":"import axios from \"axios\";\nimport { object } from 'yup/lib/locale';\nexport default {\n  name: 'NoticeDetailPage',\n  props: {\n    id: Number\n  },\n\n  data() {\n    return {\n      detail: object\n    };\n  },\n\n  methods: {\n    // Notice Detail 데이터 가져오기\n    NoticeDetail() {\n      axios.get(\"/notice/detail?id=\" + this.id).then(response => {\n        console.log(response.data);\n        this.detail = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    // Notice 삭제하기\n    NoticeDelete(notice_id) {\n      axios.delete(\"/noticedelete/\" + notice_id).then(response => {\n        if (response.status) {\n          alert(\"게시물 삭제 성공\");\n          this.$router.push(\"/noticepage\");\n        } else {\n          alert(\"게시물 삭제 실패\");\n        }\n      }).catch(e => {\n        console.log(e);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.NoticeDetail();\n  }\n\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,MAAT,QAAuB,gBAAvB;AAEA,eAAe;AACbC,MAAI,EAAE,kBADO;AAGbC,OAAK,EAAE;AACLC,MAAE,EAAEC;AADC,GAHM;;AAObC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAEN;AADH,KAAP;AAGD,GAXY;;AAabO,SAAO,EAAC;AACN;AACAC,gBAAY,GAAE;AACZT,WAAI,CACDU,GADH,CACO,uBAAqB,KAAKN,EADjC,EAEGO,IAFH,CAEQC,QAAO,IAAK;AAChBC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACN,IAArB;AACA,aAAKC,MAAL,GAAcK,QAAQ,CAACN,IAAvB;AACD,OALH,EAMGS,KANH,CAMSC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OARH;AASD,KAZK;;AAcN;AACAC,gBAAY,CAACC,SAAD,EAAW;AACrBlB,WAAI,CACDmB,MADH,CACU,mBAAiBD,SAD3B,EAEGP,IAFH,CAEQC,QAAO,IAAK;AAChB,YAAGA,QAAQ,CAACQ,MAAZ,EAAmB;AACfC,eAAK,CAAC,WAAD,CAAL;AACA,eAAKC,OAAL,CAAaC,IAAb,CAAkB,aAAlB;AACJ,SAHA,MAGO;AACHF,eAAK,CAAC,WAAD,CAAL;AACJ;AACD,OATH,EASKN,KATL,CASWC,KAAK;AACZH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OAXH;AAYD;;AA5BK,GAbK;;AA6CbQ,SAAO,GAAG;AACR,SAAKf,YAAL;AACD;;AA/CY,CAAf","names":["axios","object","name","props","id","Number","data","detail","methods","NoticeDetail","get","then","response","console","log","catch","e","NoticeDelete","notice_id","delete","status","alert","$router","push","mounted"],"sourceRoot":"","sources":["C:\\eclipse_portable\\WORKSPACE\\javaram\\vue\\src\\components\\notice\\page\\NoticeDetailPage.vue"],"sourcesContent":["<template>\r\n\r\n    <div>\r\n        <h3>{{ detail.notice_title }}</h3>\r\n        <hr>\r\n        <div class=\"notice_text_tab\">\r\n            <div id=\"notice_text_info\">\r\n              <h6>작성자 : {{ detail.user_id }}</h6>\r\n              <h6>작성일 : {{ detail.notice_date }}</h6>\r\n            </div>\r\n            \r\n            <!-- 관리자 권한을 가졌을 경우에만 실행 가능 -->\r\n            <button id=\"deleteBtn\" type=\"button\" class=\"btn btn-danger\" @click=\"NoticeDelete(detail.notice_id)\">Delete</button>\r\n        </div>\r\n        <hr>\r\n        <p>{{ detail.notice_content }}</p>\r\n\r\n    </div>\r\n\r\n</template>\r\n\r\n\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { object } from 'yup/lib/locale';\r\n\r\nexport default {\r\n  name: 'NoticeDetailPage',   \r\n\r\n  props: {\r\n    id: Number\r\n  },\r\n  \r\n  data() {\r\n    return {\r\n      detail: object,\r\n    }\r\n  },\r\n\r\n  methods:{\r\n    // Notice Detail 데이터 가져오기\r\n    NoticeDetail(){\r\n      axios\r\n        .get(\"/notice/detail?id=\"+this.id)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.detail = response.data;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        })\r\n    },\r\n\r\n    // Notice 삭제하기\r\n    NoticeDelete(notice_id){\r\n      axios\r\n        .delete(\"/noticedelete/\"+notice_id)\r\n        .then(response => {\r\n          if(response.status){\r\n              alert(\"게시물 삭제 성공\")\r\n              this.$router.push(\"/noticepage\")\r\n          } else {\r\n              alert(\"게시물 삭제 실패\")\r\n          }\r\n        }).catch(e => {\r\n          console.log(e);\r\n        })\r\n    },\r\n\r\n  },\r\n\r\n  mounted() {\r\n    this.NoticeDetail();\r\n  },\r\n\r\n}\r\n    \