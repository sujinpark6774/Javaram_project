\">\n    <CalendarComponent :attributes='attrs' is-expanded />\n\n    <div class=\"todos\">\n      <div class=\"todo-left\">\n        <div class=\"todo-tab\">\n          <h4>To-do</h4>\n          <button type=\"button\" class=\"addBtn btn btn-primary btn-sm\" data-bs-toggle=\"modal\"\n            data-bs-target=\"#staticBackdrop\">추가</button>\n        </div>\n        <todo-list />\n      </div>\n\n      <div class=\"todo-right\">\n        <h4>Detail</h4>\n        <todo-detail />\n      </div>\n    </div>\n  </div>\n\n  <!-- Modal -->\n  <div class=\"modal fade\" id=\"staticBackdrop\" data-bs-backdrop=\"static\" data-bs-keyboard=\"false\" tabindex=\"-1\"\n    aria-labelledby=\"staticBackdropLabel\" aria-hidden=\"true\">\n    <div class=\"modal-dialog\">\n      <div class=\"modal-content\">\n        <div class=\"modal-header\">\n          <h5 class=\"modal-title\" id=\"staticBackdropLabel\">일정 추가</h5>\n          <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n        </div>\n        <div class=\"modal-body\">\n          <label for=\"title\">일정 제목</label>\n          <input class=\"form-control\" type=\"text\" placeholder=\"제목을 입력해주세요\">\n          <label for=\"startDate\">시작 일자</label>\n          <input id=\"startDate\" class=\"form-control\" type=\"date\" />\n          <label for=\"endDate\">종료 일자</label>\n          <input id=\"endDate\" class=\"form-control\" type=\"date\" />\n          <div class=\"mb-3\">\n            <label for=\"memo\" class=\"form-label\">메모</label>\n            <textarea class=\"form-control\" id=\"memo\" rows=\"3\"></textarea>\n          </div>\n          <input type=\"datetime\" class=\"form-control\" placeholder=\"datetime input\">\n           <input type=\"datetime-local\" class=\"form-control\" placeholder=\"datetime-local input\">\n        </div>\n        <div class=\"modal-footer\">\n          <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">취소</button>\n          <button type=\"button\" class=\"btn btn-primary\">등록</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import TodoList from \"../component/TodoList.vue\";\n  import TodoDetail from \"../component/TodoDetail.vue\";\n\n  export default {\n    components: {\n      TodoList,\n      TodoDetail,\n    },\n    name: \"TodoPage\",\n    methods: {},\n    data() {\n      return {\n        attrs: [{\n          key: 'today',\n          highlight: 'blue',\n          dates: new Date(),\n        }, ],\n      };\n    },\n  }\n</script>\n\n<style scoped>\n  .wrap {\n    display: flex;\n    flex-direction: column;\n  }\n\n  .vc-container {\n    width: 50%;\n  }\n\n  .todos {\n    flex: 1;\n    margin-top: 20px;\n    display: flex;\n    flex-direction: row;\n  }\n\n  .todo-tab {\n    position: relative;\n  }\n\n  .addBtn {\n    position: absolute;\n    top: 0;\n    right: 0;\n  }\n\n  .todo-left {\n    flex: 1;\n  }\n\n  .todo-right {\n    flex: 1;\n    margin-left: 20px;\n  }\n\n  .list-group {\n    height: 200px;\n    overflow-y: scroll;\n  }\n\n  .todo_detail {\n    height: 200px;\n    overflow-y: scroll;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}