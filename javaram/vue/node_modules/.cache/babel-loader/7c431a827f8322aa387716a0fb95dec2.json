{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: 'NoticeList',\n\n  data() {\n    return {\n      num: 0,\n      notices: [] //   url: '/notice/5',\n\n    };\n  },\n\n  props: [\"num\"],\n  methods: {\n    NoticeData() {\n      axios.get(\"/notice\").then(response => {\n        console.log(response.data);\n        this.notices = response.data;\n        this.num = length(this.notices);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    noticeCont() {\n      const urlid = new FormData();\n      urlid.append('urlid', this.notice_id);\n    }\n\n  },\n\n  mounted() {\n    this.NoticeData();\n  }\n\n}; // export default {\n//     name: \"NoticeList\",\n//     data() {\n//         return {\n//             url : '/notice/5',\n//         };\n//     },\n//     props: [\"num\"],\n// };","map":{"version":3,"mappings":"AAmCA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,YADO;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,SAAG,EAAE,CADA;AAELC,aAAO,EAAE,EAFJ,CAGP;;AAHO,KAAP;AAKD,GATY;;AAWbC,OAAK,EAAE,CAAC,KAAD,CAXM;AAabC,SAAO,EAAC;AACNC,cAAU,GAAE;AACVP,WAAI,CACDQ,GADH,CACO,SADP,EACkBC,IADlB,CACuBC,QAAO,IAAK;AAC/BC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACR,IAArB;AACA,aAAKE,OAAL,GAAeM,QAAQ,CAACR,IAAxB;AACA,aAAKC,GAAL,GAAWU,MAAM,CAAC,KAAKT,OAAN,CAAjB;AACD,OALH,EAMGU,KANH,CAMSC,KAAK;AACVJ,eAAO,CAACC,GAAR,CAAYG,CAAZ;AACD,OARH;AASD,KAXK;;AAaNC,cAAU,GAAE;AACR,YAAMC,KAAI,GAAI,IAAIC,QAAJ,EAAd;AACAD,WAAK,CAACE,MAAN,CAAa,OAAb,EAAsB,KAAKC,SAA3B;AAEJ;;AAjBM,GAbK;;AAiCbC,SAAO,GAAE;AACP,SAAKd,UAAL;AACF;;AAnCa,CAAf,C,CAuCI;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","names":["axios","name","data","num","notices","props","methods","NoticeData","get","then","response","console","log","length","catch","e","noticeCont","urlid","FormData","append","notice_id","mounted"],"sourceRoot":"","sources":["C:\\eclipse_portable\\WORKSPACE\\javaram\\vue\\src\\components\\notice\\component\\NoticeList.vue"],"sourcesContent":["<template>\n\n  <table class=\"table\">\n\n    <thead>\n     