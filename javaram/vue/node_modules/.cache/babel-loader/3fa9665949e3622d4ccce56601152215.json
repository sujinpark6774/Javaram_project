{"ast":null,"code":"import { Field, ErrorMessage } from \"vee-validate\";\nimport * as yup from \"yup\";\nimport axios from 'axios';\nexport default {\n  name: \"SearchID\",\n  methods: {\n    FindID() {\n      axios.post('/FindUserID', {\n        user_name: this.user_name,\n        user_email: this.user_email\n      }).then(response => {\n        if (response.data) {\n          alert(\"회원님의 ID은(는)\" + response.data + \"입니다.\");\n        } else {\n          alert(\"조회하신 정보의 회원 ID가 없습니다.\");\n        }\n      });\n    }\n\n  },\n  components: {\n    // Form,\n    Field,\n    ErrorMessage\n  },\n\n  data() {\n    const schema = yup.object().shape({\n      username: yup.string().required(\"Username is required!\").min(3, \"Must be at least 3 characters!\").max(20, \"Must be maximum 20 characters!\"),\n      id: yup.string().required(\"Id is required!\").min(6, \"Must be at least 6 characters!\").max(40, \"Must be maximum 40 characters!\"),\n      email: yup.string().required(\"Email is required!\").email(\"Email is invalid!\").max(50, \"Must be maximum 50 characters!\")\n    });\n    return {\n      successful: false,\n      loading: false,\n      message: \"\",\n      schema\n    };\n  },\n\n  computed: {}\n};","map":{"version":3,"mappings":"AAkCA,SAAUA,KAAV,EAAiBC,YAAjB,QAAqC,cAArC;AACA,OAAO,KAAKC,GAAZ,MAAqB,KAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAGbC,SAAO,EAAC;AACNC,UAAM,GAAE;AACNH,WAAK,CAACI,IAAN,CAAW,aAAX,EAA0B;AACtBC,iBAAQ,EAAI,KAAKA,SADK;AAEtBC,kBAAS,EAAI,KAAKA;AAFI,OAA1B,EAGGC,IAHH,CAGQC,QAAO,IAAK;AAClB,YAAGA,QAAQ,CAACC,IAAZ,EAAiB;AACfC,eAAK,CAAC,gBAAgBF,QAAQ,CAACC,IAAzB,GAAgC,MAAjC,CAAL;AACD,SAFD,MAEK;AACHC,eAAK,CAAC,uBAAD,CAAL;AACF;AACD,OATD;AAUF;;AAZM,GAHK;AAkBbC,YAAU,EAAE;AACV;AACAd,SAFU;AAGVC;AAHU,GAlBC;;AAuBbW,MAAI,GAAG;AACL,UAAMG,MAAK,GAAIb,GAAG,CAACc,MAAJ,GAAaC,KAAb,CAAmB;AAChCC,cAAQ,EAAEhB,GAAE,CACTiB,MADO,GAEPC,QAFO,CAEE,uBAFF,EAGPC,GAHO,CAGH,CAHG,EAGA,gCAHA,EAIPC,GAJO,CAIH,EAJG,EAIC,gCAJD,CADsB;AAMhCC,QAAE,EAAErB,GAAE,CACHi