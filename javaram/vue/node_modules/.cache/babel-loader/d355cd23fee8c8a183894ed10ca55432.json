{"ast":null,"code":"import axios from \"axios\";\nimport TodoCalendar from \"../component/TodoCalendar.vue\"; // import TodoList from \"../component/TodoList.vue\";\n\nimport TodoDetail from \"../component/TodoDetail.vue\";\nexport default {\n  name: \"TodoPage\",\n  components: {\n    TodoCalendar,\n    // TodoList,\n    TodoDetail\n  },\n\n  data() {\n    return {\n      attrs: [{\n        key: 'today',\n        highlight: 'blue',\n        dates: new Date()\n      }],\n      date: \"\",\n      user: \"sujin\",\n      todos: []\n    };\n  },\n\n  methods: {\n    clickdateC(date) {\n      // Calendar에서 선택한 날짜 받음\n      this.date = date.id; // (user, 날짜 기준) todo 데이터 가져옴\n\n      axios.get(`/todo/detail?user=${this.user}&tododate=${date.id}`).then(response => {\n        console.log(response.data);\n        this.todos = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,YAAP,MAAyB,+BAAzB,C,CACA;;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAGbC,YAAU,EAAE;AACVH,gBADU;AAEV;AACAC;AAHU,GAHC;;AASbG,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,CAAC;AACNC,WAAG,EAAE,OADC;AAENC,iBAAS,EAAE,MAFL;AAGNC,aAAK,EAAE,IAAIC,IAAJ;AAHD,OAAD,CADF;AAMLC,UAAI,EAAE,EAND;AAOLC,UAAI,EAAE,OAPD;AAQLC,WAAK,EAAE;AARF,KAAP;AAUD,GApBY;;AAsBbC,SAAO,EAAE;AAEPC,cAAU,CAACJ,IAAD,EAAO;AACf;AACA,WAAKA,IAAL,GAAUA,IAAI,CAACK,EAAf,CAFe,CAGf;;AACAhB,WAAI,CACDiB,GADH,CACQ,qBAAoB,KAAKL,IAAK,aAAYD,IAAI,CAACK,EAAG,EAD1D,EAEGE,IAFH,CAEQC,QAAO,IAAK;AAChBC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACd,IAArB;AACA,aAAKQ,KAAL,GAAaM,QAAQ,CAACd,IAAtB;AACD,OALH,EAMGiB,KANH,CAMSC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OARH;AAUD;;AAhBM;AAtBI,CAAf","names":["axios","TodoCalendar","TodoDetail","name","components","data","attrs","key","highlight","dates","Date","date","user","todos","methods","clickdateC","id","get","then","response","console","log","catch","e"],"sourceRoot":"","sources":["C:\\eclipse_portable\\WORKSPAC