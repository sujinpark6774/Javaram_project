lPage',   \r\n\r\n  props: {\r\n    id: Number\r\n  },\r\n  \r\n  data() {\r\n    return {\r\n      detail: object,\r\n    }\r\n  },\r\n\r\n  methods:{\r\n    // Notice Detail 데이터 가져오기\r\n    NoticeDetail(){\r\n      axios\r\n        .get(\"/notice/detail?id=\"+this.id)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.detail = response.data;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        })\r\n    },\r\n\r\n    // Notice 삭제하기\r\n    NoticeDelete(notice_id){\r\n      axios\r\n        .delete(\"/noticedelete/\"+notice_id)\r\n        .then(response => {\r\n          if(response.status){\r\n              alert(\"게시물 삭제 성공\")\r\n              this.$router.push(\"/noticepage\")\r\n          } else {\r\n              alert(\"게시물 삭제 실패\")\r\n          }\r\n        }).catch(e => {\r\n          console.log(e);\r\n        })\r\n    },\r\n\r\n    NoticeUpdate(detail_id) {\r\n      this.$router.push({path: `/noticedetailpage/${detail_id}`});\r\n    },\r\n\r\n  },\r\n\r\n  mounted() {\r\n    this.NoticeDetail();\r\n  },\r\n\r\n}\r\n    \r\n</script>\r\n\r\n\r\n\r\n\r\n\r\n<style scoped>\r\n  h3,\r\n  h6 {\r\n      text-align: left;\r\n  }\r\n\r\n  .notice_text_tab {\r\n      display: flex;\r\n      flex-direction: row;\r\n  }\r\n\r\n  #notice_text_info {\r\n      flex: 10;\r\n      align-self: center;\r\n      margin: 0;\r\n  }\r\n\r\n  #deleteBtn {\r\n      margin-left: 20px;\r\n      flex: 1;\r\n  }\r\n</style>"]},"metadata":{},"sourceType":"module"}