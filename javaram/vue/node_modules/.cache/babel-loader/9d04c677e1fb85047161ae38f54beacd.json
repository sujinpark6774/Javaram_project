{"ast":null,"code":"import axios from \"axios\";\nimport { object } from 'yup/lib/locale';\nexport default {\n  name: 'NoticeUpdatePage',\n  props: {\n    id: Number\n  },\n\n  data() {\n    return {\n      detail: object\n    };\n  },\n\n  methods: {\n    // Notice Detail 데이터 가져오기\n    NoticeDetail() {\n      axios.get(\"/notice/detail?id=\" + this.id).then(response => {\n        console.log(response.data);\n        this.detail = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    // Notice Detail 수정하기\n    NoticeUpdate() {\n      axios.put(\"/noticeupdate/\" + this.id, {\n        notice_id: this.id,\n        notice_title: this.notice_title,\n        notice_content: this.notice_content\n      }).then(response => {\n        if (response.status) {\n          alert(\"공지사항 수정 성공\");\n          this.$router.push(\"/noticepage\");\n        } else {\n          alert(\"공지사항 수정 실패\");\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.NoticeDetail();\n  }\n\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,MAAT,QAAuB,gBAAvB;AAEA,eAAe;AACbC,MAAI,EAAE,kBADO;AAGbC,OAAK,EAAE;AACLC,MAAE,EAAEC;AADC,GAHM;;AAObC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAEN;AADH,KAAP;AAGD,GAXY;;AAabO,SAAO,EAAC;AACN;AACAC,gBAAY,GAAE;AACZT,WAAI,CACDU,GADH,CACO,uBAAqB,KAAKN,EADjC,EAEGO,IAFH,CAEQC,QAAO,IAAK;AAChBC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACN,IAArB;AACA,aAAKC,MAAL,GAAcK,QAAQ,CAACN,IAAvB;AACD,OALH,EAMGS,KANH,CAMSC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OARH;AASD,KAZK;;AAcN;AACAC,gBAAY,GAAG;AACXjB,WAAI,CACCkB,GADL,CACS,mBAAiB,KAAKd,EAD/B,EACmC;AAC3Be,iBAAS,EAAE,KAAKf,EADW;AAE3BgB,oBAAY,EAAE,KAAKA,YAFQ;AAG3BC,sBAAc,EAAE,KAAKA;AAHM,OADnC,EAKOV,IALP,CAKYC,QAAO,IAAK;AAChB,YAAGA,QAAQ,CAACU,MAAZ,EAAmB;AACfC,eAAK,CAAC,YAAD,CAAL;AACA,eAAKC,OAAL,CAAaC,IAAb,CAAkB,aAAlB;AACJ,SAHA,MAGO;AACHF,eAAK,CAAC,YAAD,CAAL;AACJ;AACH,OAZL,EAYOR,KAZP,CAYaW,GAAE,IAAK;AACZb,eAAO,CAACC,GAAR,CAAYY,GAAZ;AACH,O