\r\n    }\r\n  },\r\n\r\n  methods:{\r\n    // Notice Detail 데이터 가져오기\r\n    NoticeDetail(){\r\n      axios\r\n        .get(\"/notice/detail?id=\"+this.id)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.detail = response.data;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        })\r\n    },\r\n\r\n    // Notice Detail 수정하기\r\n    NoticeUpdate() {\r\n        axios\r\n            .put(\"/noticeupdate/\"+this.id, {\r\n                notice_id: this.id,\r\n                notice_title: this.notice_title,\r\n                notice_content: this.notice_content,\r\n            }).then(response => {\r\n                if(response.status){\r\n                    alert(\"공지사항 수정 성공\")\r\n                    this.$router.push(\"/noticepage\")\r\n                } else {\r\n                    alert(\"공지사항 수정 실패\")\r\n                }\r\n            }).catch(err => {\r\n                console.log(err)\r\n            })\r\n    },\r\n\r\n  },\r\n\r\n  mounted() {\r\n    this.NoticeDetail();\r\n  },\r\n\r\n}\r\n    \r\n</script>\r\n\r\n<style scoped>\r\n\r\n  .input-group-text {\r\n    text-align: left;\r\n  }\r\n\r\n  .notice-update-wrap {\r\n    margin-left: 80px;\r\n  }\r\n\r\n  #notice-update-fixed {\r\n    text-align: left;\r\n  }\r\n\r\n  #notice-update-content {\r\n    text-align: left;\r\n    height: 400px;\r\n  }\r\n\r\n  /* .notice-update-btn {\r\n    align-items: right;\r\n  } */\r\n\r\n  #updateBtn {\r\n      flex: 1;\r\n  }\r\n</style>"]},"metadata":{},"sourceType":"module"}