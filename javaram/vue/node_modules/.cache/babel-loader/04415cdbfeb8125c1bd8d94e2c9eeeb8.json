{"ast":null,"code":"import axios from \"axios\";\nimport NoticeDetailPage from \"../page/NoticeDetailPage.vue\";\nexport default {\n  name: 'NoticeList',\n  component: {\n    NoticeDetailPage\n  },\n\n  data() {\n    return {\n      notices: [],\n      tmp: \"Success\"\n    };\n  },\n\n  methods: {\n    // Notice 테이블 데이터 가져오기\n    NoticeList() {\n      axios.get(\"/notice\").then(response => {\n        console.log(response.data);\n        this.notices = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    // 클릭 이벤트(NoticeDeatil 페이지 이동)\n    sendToSibling() {\n      this.emiiter.emit(\"sibling\", this.user);\n    }\n\n  },\n\n  created() {\n    this.emitter.on('test', this.onReceive);\n  },\n\n  mounted() {\n    this.NoticeList();\n  }\n\n};","map":{"version":3,"mappings":"AAoCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,gBAAP,MAA6B,8BAA7B;AAEA,eAAe;AACbC,MAAI,EAAE,YADO;AAGbC,WAAS,EAAE;AACTF;AADS,GAHE;;AAObG,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE,EADJ;AAELC,SAAG,EAAE;AAFA,KAAP;AAID,GAZY;;AAcbC,SAAO,EAAC;AAEN;AACAC,cAAU,GAAE;AACVR,WAAI,CACDS,GADH,CACO,SADP,EAEGC,IAFH,CAEQC,QAAO,IAAK;AAChBC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACP,IAArB;AACA,aAAKC,OAAL,GAAeM,QAAQ,CAACP,IAAxB;AACD,OALH,EAMGU,KANH,CAMSC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OARH;AASD,KAbK;;AAeN;AACAC,iBAAa,GAAG;AACd,WAAKC,OAAL,CAAaC,IAAb,CAAkB,SAAlB,EAA6B,KAAKC,IAAlC;AACD;;AAlBK,GAdK;;AAqCbC,SAAO,GAAG;AACR,SAAKC,OAAL,CAAaC,EAAb,CAAgB,MAAhB,EAAwB,KAAKC,SAA7B;AACD,GAvCY;;AA2CbC,SAAO,GAAE;AACP,SAAKhB,UAAL;AACD;;AA7CY,CAAf","names":["axios","NoticeDetailPage","name","component","data","notices","tmp","methods","NoticeList","get","then","response","console","log","catch","e","sendToSibling","emiiter","emit","user","created","emitter","on","onReceive","mounted"],"sourceRoot":"","sources":["C:\\eclipse_portable\\WORKSPACE\\javaram\\vue\\src\\components\\notice\\component\\NoticeList.vue"],"sourcesContent":["<template>\n\n  <table class=\"table\">\n\n    <thead>\n        <tr>\n            <th scope=\"col\">No.</th>\n            <th scope=\"col\">Title</th>\n            <th scope=\"col\">Writer</th>\n            <th scope=\"col\">Date</th>\n        </tr>\n    </thead>\n\n    <tbody>\n\n      <!-- <tr v-for=\"(notice,idx) in notices\" :key=\"idx\" @click=\"NoticeClick()\"> -->\n      <tr v-for=\"(notice,idx) in notices\" :key=\"idx\" @click=\"sendToSibling\">\n            <th scope=\"row\">{{idx + 1}}</th>\n            <td>{{notice.notice_title}}</td>\n            <td>{{notice.user_id}}</td>\n            <td>{{notice.notice_date}}</td>\n\n            <div v-show=\"false\"><NoticeDetailPage v-bind:num=\"tmp\" /></div>\n      </tr>\n\n    </tbody>    \n\n  </table>\n\n\n</template>\n\n\n\n\n<script>\nimport axios from \"axios\";\nimport NoticeDetailPage from \"../page/NoticeDetailPage.vue\";\n\nexport default {\n  name: 'NoticeList',\n\n  component: {\n    NoticeDetailPage\n  },\n\n  data() {\n    return {\n      notices: [],\n      tmp: \"Success\",\n    }\n  },\n\n  methods:{\n\n    // Notice 테이블 데이터 가져오기\n    NoticeList(){\n      axios\n        .get(\"/notice\")\n        .then(response => {\n          console.log(response.data);\n          this.notices = response.data;\n        })\n        .catch(e => {\n          console.log(e);\n        })\n    },\n\n    // 클릭 이벤트(NoticeDeatil 페이지 이동)\n    sendToSibling() {\n      this.emiiter.emit(\"sibling\", this.user)\n    },\n\n\n  },\n\n  created() {\n    this.emitter.on('test', this.onReceive);\n  },\n\n\n\n  mounted(){\n    this.NoticeList();\n  },\n\n}\n    \n</script>\n\n\n\n<style scoped>\n    tbody tr:hover {\n        background-color: rgb(238, 238, 238);\n    }\n</style>"]},"metadata":{},"sourceType":"module"}