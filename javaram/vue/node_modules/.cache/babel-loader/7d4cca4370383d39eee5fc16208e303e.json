{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: 'NoticeList',\n\n  data() {\n    return {\n      num: 0,\n      notices: []\n    };\n  },\n\n  methods: {\n    NoticeData() {\n      axios.get(\"/notice\").then(response => {\n        console.log(response.data);\n        this.notices = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.NoticeData();\n  },\n\n  filters: {\n    // filter로 쓸 filter ID 지정\n    yyyyMMdd: function (value) {\n      // 들어오는 value 값이 공백이면 그냥 공백으로 돌려줌\n      if (value == '') return ''; // 현재 Date 혹은 DateTime 데이터를 javaScript date 타입화\n\n      var js_date = new Date(value); // 연도, 월, 일 추출\n\n      var year = js_date.getFullYear();\n      var month = js_date.getMonth() + 1;\n      var day = js_date.getDate(); // 월, 일의 경우 한자리 수 값이 있기 때문에 공백에 0 처리\n\n      if (month < 10) {\n        month = '0' + month;\n      }\n\n      if (day < 10) {\n        day = '0' + day;\n      } // 최종 포맷 (ex - '2021-10-08')\n\n\n      return year + '-' + month + '-' + day;\n    }\n  }\n}; // export default {\n//     name: \"NoticeList\",\n//     data() {\n//         return {\n//             url : '/notice/5',\n//         };\n//     },\n//     props: [\"num\"],\n// };","map":{"version":3,"mappings":"AA+BA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,YADO;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,SAAE,EAAI,CADD;AAELC,aAAO,EAAE;AAFJ,KAAP;AAID,GARY;;AAUbC,SAAO,EAAC;AAENC,cAAU,GAAE;AACVN,WAAI,CACDO,GADH,CACO,SADP,EACkBC,IADlB,CACuBC,QAAO,IAAK;AAC/BC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACP,IAArB;AACA,aAAKE,OAAL,GAAeK,QAAQ,CAACP,IAAxB;AACD,OAJH,EAKGU,KALH,CAKSC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OAPH;AAQD;;AAXK,GAVK;;AAwBbC,SAAO,GAAE;AACP,SAAKR,UAAL;AACD,GA1BY;;AA4BbS,SAAM,EAAI;AACJ;AACPC,YAAO,EAAI,UAASC,KAAT,EAAe;AACjB;AACA,UAAGA,KAAI,IAAK,EAAZ,EAAgB,OAAO,EAAP,CAFC,CAIjB;;AACA,UAAIC,OAAM,GAAI,IAAIC,IAAJ,CAASF,KAAT,CAAd,CALiB,CAOjB;;AACA,UAAIG,IAAG,GAAIF,OAAO,CAACG,WAAR,EAAX;AACA,UAAIC,KAAI,GAA