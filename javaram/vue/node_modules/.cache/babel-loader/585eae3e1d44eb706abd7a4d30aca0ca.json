{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: 'NoticeList',\n\n  data() {\n    return {\n      notices: []\n    };\n  },\n\n  methods: {\n    // Notice 테이블 데이터 가져오기\n    NoticeList() {\n      axios.get(\"/notice\").then(response => {\n        console.log(response.data);\n        this.notices = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    NoticeClick(noticedata) {\n      this.$router.push({\n        path: `/noticedetailpage/${noticedata}`\n      });\n      this.$emit(\"detail\", noticedata); // this.emitter.emit('detail', noticedata)\n    } // NoticeClick(noticedata) {\n    //   this.$router.push('/noticedetailpage');\n    //   this.$emit(\"detail\", noticedata);\n    //   // this.emitter.emit('detail', noticedata)\n    // },\n\n\n  },\n\n  mounted() {\n    this.NoticeList();\n  }\n\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,YADO;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE;AADJ,KAAP;AAGD,GAPY;;AASbC,SAAO,EAAC;AAEN;AACAC,cAAU,GAAE;AACVL,WAAI,CACDM,GADH,CACO,SADP,EAEGC,IAFH,CAEQC,QAAO,IAAK;AAChBC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACN,IAArB;AACA,aAAKC,OAAL,GAAeK,QAAQ,CAACN,IAAxB;AACD,OALH,EAMGS,KANH,CAMSC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OARH;AASD,KAbK;;AAeNC,eAAW,CAACC,UAAD,EAAa;AACtB,WAAKC,OAAL,CAAaC,IAAb,CAAkB;AAACC,YAAI,EAAG,qBAAoBH,UAAW;AAAvC,OAAlB;AACA,WAAKI,KAAL,CAAW,QAAX,EAAqBJ,UAArB,EAFsB,CAItB;AACD,KApBK,CAsBN;AACA;AACA;AAEA;AACA;;;AA3BM,GATK;;AAwCbK,SAAO,GAAG;AACR,SAAKd,UAAL;AACD;;AA1CY,CAAf","names":["axios","name","data","notices","methods","NoticeList","get","then","response","console","log","catch","e","NoticeClick","noticedata","$router","push","path","$emit","mounted"],"sourceRoot":"","sources":["C:\\eclipse_portable\\WORKSPACE\\javaram\\vue\\src\\components\\notice\\component\\NoticeList.vue"],"sourcesContent":["<template>\n\n  <table class=\"table\">\n\n    <thead>\n        <tr>\n            <th scope=\"col\">No.</th>\n        