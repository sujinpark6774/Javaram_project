{"ast":null,"code":"import axios from \"axios\";\nimport { object } from 'yup/lib/locale';\nexport default {\n  name: 'NoticeDetailPage',\n  props: {\n    id: Number\n  },\n\n  data() {\n    return {\n      detail: object,\n      user_access: sessionStorage.getItem('access')\n    };\n  },\n\n  methods: {\n    // 뒤로가기 버튼\n    clickback() {\n      this.$router.go(-1);\n    },\n\n    // Notice Detail 데이터 가져오기\n    NoticeDetail() {\n      axios.get(\"/notice/detail?id=\" + this.id).then(response => {\n        console.log(response.data);\n        this.detail = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    // Notice 삭제하기\n    NoticeDelete(notice_id) {\n      Swal.fire({\n        title: \"게시물을 삭제 하시겠습니까?\",\n        type: 'warning',\n        showCancelButton: true,\n        confirmButtonText: '확인',\n        cancelButtonText: '취소',\n        showCloseButton: true,\n        showLoaderOnConfirm: true\n      }).then(result => {\n        if (result.value) {\n          // confirmButtonText의 확인 클릭 시 true 반환 \n          axios.delete(\"/noticedelete/\" + notice_id).then(response => {\n            if (response.status) {\n              alert(\"게시물 삭제 성공\");\n              this.$router.push(\"/noticepage\");\n            } else {\n              alert(\"게시물 삭제 실패\");\n            }\n          }).catch(e => {\n            console.log(e);\n          });\n        }\n      });\n    },\n\n    NoticeUpdate(detail_id) {\n      this.$router.push({\n        path: `/noticeupdatepage/${detail_id}`\n      });\n    }\n\n  },\n\n  mounted() {\n    this.NoticeDetail();\n  }\n\n};","map":{"version":3,"mappings":"AAsCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,MAAT,QAAuB,gBAAvB;AAEA,eAAe;AACbC,MAAI,EAAE,kBADO;AAGbC,OAAK,EAAE;AACLC,MAAE,EAAEC;AADC,GAHM;;AAObC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAEN,MADH;AAELO,iBAAU,EAAIC,cAAc,CAACC,OAAf,CAAuB,QAAvB;AAFT,KAAP;AAID,GAZY;;AAcbC,SAAO,EAAC;AAEN;AACAC,aAAS,GAAE;AACT,WAAKC,OAAL,CAAaC,EAAb,CAAgB,CAAC,CAAjB;AACD,KALK;;AAON;AACAC,gBAAY,GAAE;AACZf,WAAI,CACDgB,GADH,CACO,uBAAqB,KAAKZ,EADjC,EAEGa,IAFH,CAEQC,QAAO,IAAK;AAChBC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACZ,IAArB;AACA,aAAKC,MAAL,GAAcW,QAAQ,CAACZ,IAAvB;AACD,OALH,EAMGe,KANH,CAMSC,KAAK;AACVH,eAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,OARH;AASD,KAlBK;;AAoBN;AACAC,gBAAY,CAACC,SAAD,EAAW;AACrBC,UAAI,CAACC,IAAL,CAAU;AACRC,aAAK,EAAE,iBADC;AAERC,YAAI,EAAE,SAFE;AAGRC,wBAAgB,EAAE,IAHV;AAIRC,yBAAiB,EAAE,IAJX;AAKRC,wBAAgB,EAAE,IALV;AAMRC,uBAAe,EAAE,IANT;AAORC,2BAAmB,EAAE;AAPb,OAAV,EAQGhB,IARH,CAQSiB,MAAD,IAAY;AAClB,YAAIA,MAAM,CAACC,KAAX,EAAkB;AAAE;AAClBnC,eAAI,CACDoC,MADH,CACU,mBAAiBZ,SAD3B,EAEGP,IAFH,CAEQC,QAAO,IAAK;AAChB,gBAAGA,QAAQ,CAACmB,MAAZ,EAAmB;AACfC,mBAAK,CAAC,WAAD,CAAL;AACA,mBAAKzB,OAAL,CAAa0B,IAAb,CAAkB,aAAlB;AACJ,aAHA,MAGO;AACHD,mBAAK,CAAC,WAAD,CAAL;AACJ;AACD,WATH,EASKjB,KATL,CASWC,KAAK;AACZH,mBAAO,CAACC,GAAR,CAAYE,CAAZ;AACD,WAXH;AAYF;AACD,OAvBD;AAwBD,KA9CK;;AAgDNkB,gBAAY,CAACC,SAAD,EAAY;AACtB,WAAK5B,OAAL,CAAa0B,IAAb,CAAkB;AAACG,YAAI,EAAG,qBAAoBD,SAAU;AAAtC,OAAlB;AACD;;AAlDK,GAdK;;AAoEbE,SAAO,GAAG;AACR,SAAK5B,YAAL;AACD;;AAtEY,CAAf","names":["axios","object","name","props","id","Number","data","detail","user_access","sessionStorage","getItem","methods","clickback","$router","go","NoticeDetail","get","then","response","console","log","catch","e","NoticeDelete","notice_id","Swal","fire","title","type","showCancelButton","confirmButtonText","cancelButtonText","showCloseButton","showLoaderOnConfirm","result","value","delete","status","alert","push","NoticeUpdate","detail_id","path","mounted"],"sourceRoot":"","sources":["C:\\eclipse_portable\\WORKSPACE\\javaram\\vue\\src\\components\\notice\\page\\NoticeDetailPage.vue"],"sourcesContent":["<template>\r\n\r\n  <div id=\"notice_detail_wrap\">\r\n\r\n      <div id=\"notice_tab\">\r\n        <h3 id=\"notice_title\">Notice</h3>\r\n      </div>\r\n\r\n      <div class=\"notice-detail-box\">\r\n    \r\n        <div id=\"notice_detail_title\"> \r\n          <h3>{{ detail.notice_title }}</h3>\r\n        </div>\r\n\r\n        <hr>\r\n\r\n        <div class=\"notice_text_tab\">\r\n            <div id=\"notice_text_info\">\r\n              <h6>작성자 : {{ detail.user_id }}</h6>\r\n              <h6>작성일 : {{ detail.notice_date }}</h6>\r\n            </div>\r\n            \r\n            <!-- 관리자 권한을 가졌을 경우에만 실행 가능 -->\r\n            <button id=\"updateBtn\" type=\"button\" class=\"btn btn-primary\" v-if=\"this.user_access == 'true'\" @click=\"NoticeUpdate(detail.notice_id)\">수정</button>\r\n            <button id=\"deleteBtn\" type=\"button\" class=\"btn btn-danger\" v-if=\"this.user_access == 'true'\" @click=\"NoticeDelete(detail.notice_id)\">삭제</button>\r\n        </div>\r\n\r\n        <hr>\r\n\r\n        <p>{{ detail.notice_content }}</p>\r\n\r\n      </div>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { object } from 'yup/lib/locale';\r\n\r\nexport default {\r\n  name: 'NoticeDetailPage',   \r\n\r\n  props: {\r\n    id: Number\r\n  },\r\n  \r\n  data() {\r\n    return {\r\n      detail: object,\r\n      user_access : sessionStorage.getItem('access'),\r\n    }\r\n  },\r\n\r\n  methods:{\r\n\r\n    // 뒤로가기 버튼\r\n    clickback(){\r\n      this.$router.go(-1);\r\n    },\r\n\r\n    // Notice Detail 데이터 가져오기\r\n    NoticeDetail(){\r\n      axios\r\n        .get(\"/notice/detail?id=\"+this.id)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.detail = response.data;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        })\r\n    },\r\n\r\n    // Notice 삭제하기\r\n    NoticeDelete(notice_id){\r\n      Swal.fire({ \r\n        title: \"게시물을 삭제 하시겠습니까?\",\r\n        type: 'warning', \r\n        showCancelButton: true, \r\n        confirmButtonText: '확인', \r\n        cancelButtonText: '취소', \r\n        showCloseButton: true, \r\n        showLoaderOnConfirm: true, \r\n      }).then((result) => { \r\n        if (result.value) { // confirmButtonText의 확인 클릭 시 true 반환 \r\n          axios\r\n            .delete(\"/noticedelete/\"+notice_id)\r\n            .then(response => {\r\n              if(response.status){\r\n                  alert(\"게시물 삭제 성공\")\r\n                  this.$router.push(\"/noticepage\")\r\n              } else {\r\n                  alert(\"게시물 삭제 실패\")\r\n              }\r\n            }).catch(e => {\r\n              console.log(e);\r\n            })\r\n        } \r\n      })\r\n    },\r\n\r\n    NoticeUpdate(detail_id) {\r\n      this.$router.push({path: `/noticeupdatepage/${detail_id}`});\r\n    },\r\n\r\n  },\r\n\r\n  mounted() {\r\n    this.NoticeDetail();\r\n  },\r\n\r\n}\r\n    \r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n  h3,\r\n  h6 {\r\n      text-align: left;\r\n  }\r\n\r\n  #notice_tab {\r\n    border-left-style: solid;\r\n    border-left-width: 5px;\r\n    border-left-color: #00C9BE;\r\n    margin-bottom: 30px;\r\n  }\r\n\r\n  #notice_title {\r\n    margin-left: 15px;\r\n    align-self: center;\r\n    flex: 6;\r\n  }\r\n\r\n\r\n  #notice_detail_wrap {\r\n    margin-left: 150px;\r\n  }\r\n\r\n  #notice_detail_title {\r\n    font-size: 1.4rem;\r\n  }\r\n\r\n  .notice_text_tab {\r\n      display: flex;\r\n      flex-direction: row;\r\n  }\r\n\r\n  #notice_text_info {\r\n      flex: 10;\r\n      align-self: center;\r\n      margin-top: 10px;\r\n  }\r\n\r\n  #updateBtn {\r\n      margin-left: 20px;\r\n      margin-top: 12px;\r\n      flex: 1;\r\n      height: 40px;\r\n  }\r\n\r\n  #deleteBtn {\r\n      margin-left: 20px;\r\n      margin-top: 12px;\r\n      flex: 1;\r\n      height: 40px;\r\n  }\r\n</style>"]},"metadata":{},"sourceType":"module"}