{"ast":null,"code":"import axios from \"axios\"; // import MailSend from './MailSend.vue'\n// import MailDetailPageVue from './MailDetailPage.vue';\n\nexport default {\n  name: \"MailPage\",\n  data: () => {\n    return {\n      receiveV: true,\n      sendV: false,\n      binV: false,\n      mailData: [],\n      pageCount: \"1\",\n      countMail: \"1\"\n    };\n  },\n  components: {// MailSend,\n    // MailDetailPageVue\n  },\n  props: {\n    pageNumber: {\n      Type: Number,\n      default: 1\n    }\n  },\n  methods: {\n    writeBtn() {\n      //window.open('./MailPage'); 메일 창 새로 띄우기\n      this.$router.push({\n        path: `/mailsend/`\n      });\n    },\n\n    receiveBtn() {\n      this.receiveV = true;\n      this.sendV = false;\n      this.binV = false;\n    },\n\n    sendBtn() {\n      this.receiveV = false;\n      this.sendV = true;\n      this.binV = false;\n    },\n\n    binBtn() {\n      this.receiveV = false;\n      this.sendV = false;\n      this.binV = true;\n    },\n\n    makeget() {\n      axios({\n        method: 'GET',\n        url: '/readMails',\n        params: {\n          user: \"test\",\n          boxName: \"inbox\",\n          pageNumber: this.pageNumber\n        }\n      }).then(response => {\n        this.mailData = response.data;\n        this.num = this.mailData.length;\n        this.countMail = response.data.messageCount;\n        this.pageCount = parseInt(this.countMail / 10 + 1);\n        console.log(response.data.messageCount);\n        console.log(this.mailData);\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    countGet() {\n      axios({\n        method: 'GET',\n        url: '/countMails',\n        params: {\n          user: \"test\",\n          boxName: \"inbox\"\n        }\n      }).then(response => {\n        this.countMail = response.data;\n        this.pageCount = parseInt(this.countMail / 10 + 1);\n        console.log(response);\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    noticeCont() {\n      const urlid = new FormData();\n      urlid.append('urlid', this.mails_id);\n    },\n\n    /* eslint-disable */\n    MailClick(mailnum) {\n      this.$router.push(`/maildetailpage/${mailnum}`);\n    }\n\n  },\n\n  mounted() {\n    this.makeget();\n  }\n\n};","map":{"version":3,"mappings":"AAwIA,OAAOA,KAAP,MAAkB,OAAlB,C,CAEA;AACA;;AACA,eAAe;AACbC,MAAI,EAAE,UADO;AAIbC,MAAI,EAAC,MAAM;AACP,WAAO;AACLC,cAAQ,EAAE,IADL;AAELC,WAAI,EAAI,KAFH;AAGLC,UAAI,EAAC,KAHA;AAILC,cAAO,EAAI,EAJN;AAKLC,eAAQ,EAAI,GALP;AAMLC,eAAQ,EAAI;AANP,KAAP;AAQD,GAbU;AAebC,YAAU,EAAC,CACT;AACA;AAFS,GAfE;AAmBbC,OAAK,EAAE;AACLC,cAAS,EAAI;AACXC,UAAG,EAAIC,MADI;AAEXC,aAAM,EAAI;AAFC;AADR,GAnBM;AA0BbC,SAAO,EAAC;AAGNC,YAAQ,GAAE;AACR;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB;AAACC,YAAI,EAAG;AAAR,OAAlB;AACD,KANK;;AAQNC,cAAU,GAAE;AACV,WAAKjB,QAAL,GAAgB,IAAhB;AACA,WAAKC,KAAL,GAAa,KAAb;AACA,WAAKC,IAAL,GAAY,KAAZ;AACD,KAZK;;AAcNgB,WAAO,GAAE;AACP,WAAKlB,QAAL,GAAgB,KAAhB;AACA,WAAKC,KAAL,GAAa,IAAb;AACA,WAAKC,IAAL,GAAY,KAAZ;AACD,KAlBK;;AAoBNiB,UAAM,GAAE;AACN,WAAKnB,QAAL,GAAgB,KAAhB;AACA,WAAKC,KAAL,GAAa,KAAb;AACA,WAAKC,IAAL,GAAY,IAAZ;AACD,KAxBK;;AA0BNkB,WAAO,GAAE;AACPvB,WAAK,CAAC;AACJwB,cAAK,EAAI,KADL;AAEJC,WAAE,EAAI,YAFF;AAGJC,cAAM,EAAE;AACNC,cAAG,EAAI,MADD;AAENC,iBAAM,EAAI,OAFJ;AAGNjB,oBAAS,EAAI,KAAKA;AAHZ;AAHJ,OAAD,CAAL,CASGkB,IATH,CASQC,QAAO,IAAK;AAChB,aAAKxB,QAAL,GAAgBwB,QAAQ,CAAC5B,IAAzB;AACA,aAAK6B,GAAL,GAAW,KAAKzB,QAAL,CAAc0B,MAAzB;AACA,aAAKxB,SAAL,GAAiBsB,QAAQ,CAAC5B,IAAT,CAAc+B,YAA/B;AACA,aAAK1B,SAAL,GAAkB2B,QAAQ,CAAC,KAAK1B,SAAL,GAAiB,EAAjB,GAAsB,CAAvB,CAA1B;AACA2B,eAAO,CAACC,GAAR,CAAYN,QAAQ,CAAC5B,IAAT,CAAc+B,YAA1B;AACAE,eAAO,CAACC,GAAR,CAAY,KAAK9B,QAAjB;AAED,OAjBH,EAkBG+B,KAlBH,CAkBS,UAAUC,KAAV,EAAiB;AACtBH,eAAO,CAACC,GAAR,CAAYE,KAAZ;AACF,OApBF;AAsBD,KAjDK;;AAmDNC,YAAQ,GAAE;AACRvC,WAAK,CAAC;AACJwB,cAAK,EAAI,KADL;AAEJC,WAAE,EAAI,aAFF;AAGJC,cAAM,EAAE;AACNC,cAAG,EAAI,MADD;AAENC,iBAAM,EAAI;AAFJ;AAHJ,OAAD,CAAL,CAQGC,IARH,CAQQC,QAAO,IAAK;AAChB,aAAKtB,SAAL,GAAiBsB,QAAQ,CAA