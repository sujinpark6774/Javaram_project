{"ast":null,"code":"import ModalTemplate from \"../../common/component/modal/ModalTemplate.vue\";\nimport axios from 'axios'; // import store from '../../../store/index.js'\n\nexport default {\n  components: {\n    ModalTemplate\n  },\n\n  data() {\n    return {\n      searchV: false\n    };\n  },\n\n  methods: {\n    search() {\n      this.searchV = true;\n    },\n\n    submit() {\n      axios.post('/loginCheck', {\n        user_id: this.user_id,\n        user_pw: this.user_pw\n      }).then(response => {\n        console.log(response.data);\n\n        if (!response.data) {\n          //로그인 실패 시\n          alert(\"아이디와 비밀번호를 확인해주세요.\");\n        } else {\n          //로그인 성공 시\n          // store.commit('setinfo', response.data)\n          sessionStorage.setItem('auth', true);\n          sessionStorage.setItem('user_name', response.data.user_name);\n          sessionStorage.setItem('user_id', response.data.user_id);\n          sessionStorage.setItem('user_email', response.data.user_email);\n          sessionStorage.setItem('access', response.data.access);\n          this.$router.push('/home');\n          this.$router.go(); // this.$router.push({path:'/', params: response.data})\n          // console.log(store.state.auth)\n          // console.log(store.state.loginInfo)\n          // console.log(store.state.loginInfo.user_id)\n        }\n      });\n    },\n\n    togoHome() {\n      if (sessionStorage.getItem('auth')) {\n        this.$router.push('/home');\n      }\n    }\n\n  },\n\n  mounted() {\n    this.togoHome();\n  }\n\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,aAAP,MAA0B,gDAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CACA;;AAEA,eAAe;AACbC,YAAS,EAAI;AACXF;AADW,GADA;;AAKZG,MAAI,GAAE;AACJ,WAAM;AACJC,aAAM,EAAI;AADN,KAAN;AAGD,GATW;;AAWZC,SAAO,EAAC;AACNC,UAAM,GAAE;AACN,WAAKF,OAAL,GAAe,IAAf;AACD,KAHK;;AAKNG,UAAM,GAAE;AACNN,WAAK,CAACO,IAAN,CAAW,aAAX,EAA0B;AACvBC,eAAM,EAAI,KAAKA,OADQ;AAEvBC,eAAM,EAAI,KAAKA;AAFQ,OAA1B,EAGGC,IAHH,CAGQC,QAAO,IAAK;AACjBC,eAAO,CAACC,GAAR,CAAYF,QA